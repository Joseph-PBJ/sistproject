package day0619;

public class OperTest_10 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		// 대입연산자 +=, -=, *=, /=, %=
		
		int a,b,c,d,e;
		a=b=c=d=e=10;
		
		a+=2; // a=a+2 ... a에 2를 더한 값이 다시 a가 된다
		b-=3;
		c*=3;
		d/=3;
		
		System.out.println("a="+a);
		
		
		
		
	}

}
